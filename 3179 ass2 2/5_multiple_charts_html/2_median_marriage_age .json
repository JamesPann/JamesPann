{     "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": { "url": "https://raw.githubusercontent.com/JamesPann/JamesPann/main/marriage-women.csv" },
    "title":{ "text":["Women Average Age at Marriage in USA between 1990 to 2017"],
    "fontSize":20,
    "anchor": "middle",
    "color":"#0f0f0fc2"
    },
    "background": "#faf6e6",
    "vconcat": [{
        "width": 450,
        "height": 300,
        "mark": "area",
        "encoding": {
            "x": {
                "field": "Year",
                "type": "temporal",
                "scale": { "domain": { "param": "brush" } },
                "axis": { "title": "", "tickCount": 10 ,
                "grid": false,
                                "labelFontSize":12 ,
                "labelColor":"#878787"
}
            },
            "y": { "field": "Estimated average age at marriage, women",
             "type": "quantitative",
             "title":"Average marriage age",
             "axis": {"grid": false,
                   "titleFontSize":14,
                "titleColor":"#0f0f0fc2", 
                 "labelFontSize":12 ,
                "labelColor":"#878787"} },
             "color": {      "legend": null,
      "field": "marriage",
      "scale": {"range": ["#fab28e"]}},
      "tooltip": [
            {"field": "Year", "type": "temporal"},
            {"field": "Estimated average age at marriage, women", "type": "quantitative", "format": ","}
          ],    "column": {"title": "Women average age at marriage in USA between 1990 to 2017"}

        }
    }, {
        "width": 450,
        "height": 100,
        "mark": "area",
        "params": [{
            "name": "brush",
            "select": { "type": "interval", "encodings": ["x"] }
        }],
        "encoding": {
            "x": {
                "field": "Year",
                "type": "temporal",
                "axis": { "tickCount": 10,
                      "titleFontSize":14,
                "titleColor":"#0f0f0fc2", 
                 "grid": false,
                                 "labelFontSize":12 ,
                "labelColor":"#878787"}

            },
            "y": {
                "field": "Estimated average age at marriage, women",
                "type": "quantitative",
                "axis": { "tickCount": 3, "grid": false,
                 "title": "",
                       "titleFontSize":14,
                "titleColor":"#0f0f0fc2", 
                "labelFontSize":12 ,
                "labelColor":"#878787"}
            },"color": {
      "field": "marriage",
      "scale": {"range": ["#fab28e"]}
    },
    "tooltip": [
            {"field": "Year", "type": "temporal"},
            {"field": "Estimated average age at marriage, women", "type": "quantitative", "format": ","}
          ]
        }
    }],
  "config": {
    "view": {
      "stroke": "transparent"
    }
  }
}